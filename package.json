{
  "name": "decker-init",
  "displayName": "Decker Init",
  "description": "Allows creation of decker projects, files and configuration from interactive templates.",
  "version": "0.0.7",
  "publisher": "decker-edu",
  "repository": {
    "type": "git",
    "url": "https://github.com/decker-edu/vscode-decker-init"
  },
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "decker-init.pick",
          "group": "decker/init",
          "when": "explorerResourceIsFolder"
        }
      ]
    },
    "commandPalette": [
      {
        "command": "decker-init.pick",
        "when": "false"
      }
    ],
    "commands": [
      {
        "command": "decker-init.pick",
        "title": "%extension.decker-init.pick.title%"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "18.x",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.23.0",
    "event-stream": "^4.0.1",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "4.7.4",
    "vsce": "^2.11.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@vscode/l10n": "^0.0.10",
    "@vscode/l10n-dev": "^0.0.19"
  }
}
